   "source": [
    "import numpy as np \n",
    "import matplotlib.pyplot as plt\n",
    "import pandas as pd\n",
    "import pandas_profiling\n",
    "\n",
    "#Training Dataset\n",
    "file='Data_Train.xlsx'\n",
    "x1=pd.ExcelFile(file)\n",
    "dataset=x1.parse('Sheet1')\n",
    "profile=dataset.profile_report(title='EDA of Training Data')\n",
    "profile.to_file(output_file="eda_data_train.html")\n",
    "y = dataset.iloc[:, -1].values\n",
    "y=y.reshape((len(y),1))\n",
    "y=np.log(y)\n",
    "dataset1=dataset.drop(\"Price\",1)\n",
    "#x = dataset.iloc[:, :-1].values\n",
    "file2='Data_Test.xlsx'\n",
    "x2=pd.ExcelFile(file2)\n",
    "test=x2.parse('Sheet1')\n",
    "#X4=test.iloc[:,11].values\n",
    "X1=pd.concat([dataset1,test], ignore_index=True)\n",
    "result = []\n",
    "\n",
    "for i in range(7253):\n",
    "    ind = \"\"\n",
    "    result.append(ind)\n",
    "\n",
    "r = pd.DataFrame(result, columns=['Model'])\n",
    "X1 = pd.concat([X1, r], axis=1)\n",
    "#X1=X1.drop(\"Arrival_Time\",1)\n",
    "\n",
    "X2=X1.iloc[:,:].values\n",
    "for q in range(7253):\n",
    "    m=(X2[q,0].split(' ',1))[0]\n",
    "    h=(X2[q,0].split(' ',1))[1]\n",
    "    X2[q,0]=m\n",
    "    X2[q,12]=h\n",
    "for q in range(7253):\n",
    "    if(str(X2[q,7])!='nan'):\n",
    "        X2[q,7]=(X2[q,7].split(' km'))[0]\n",
    "        if(X2[q,7]=='0.0'):\n",
    "            X2[q,7]=np.nan\n",
    "    else:\n",
    "        X2[q,7]=np.nan\n",
    "    if(str(X2[q,8])!='nan'):\n",
    "        X2[q,8]=(X2[q,8].split(' CC'))[0]\n",
    "    else:\n",
    "        X2[q,8]=np.nan\n",
    "    if(str(X2[q,9])!='nan'):\n",
    "        X2[q,9]=(X2[q,9].split(' bhp'))[0]\n",
    "        if(X2[q,9]=='null'):\n",
    "            X2[q,9]=np.nan\n",
    "    else:\n",
    "        X2[q,9]=np.nan\n",
    "    if(str(X2[q,11])!='nan'):\n",
    "        if(X2[q,11].find(' Lakh')>0):\n",
    "            X2[q,11]=(X2[q,11].split(' Lakh'))[0]\n",
    "        if(X2[q,11].find(' Cr')>0):\n",
    "            X2[q,11]=(X2[q,11].split(' Cr'))[0]\n",
    "            X2[q,11]=float(X2[q,11])\n",
    "            X2[q,11]=X2[q,11]*100\n",
    "    else:\n",
    "        X2[q,11]=np.nan\n",
    "    X2[q,2]=2019-X2[q,2]\n",
    "X2[:,7]=X2[:,7].astype(float)\n",
    "X2[:,8]=X2[:,8].astype(float)\n",
    "X2[:,9]=X2[:,9].astype(float)\n",
    "X2[:,11]=X2[:,11].astype(float)        \n",
    "X3=X2[:,:]\n",
    "X5=X2[6019:,2]\n",
    "X4=X2[6019:,11]\n",
    "\n",
    "from sklearn.preprocessing import Imputer \n",
    "imputer=Imputer (missing_values='NaN',strategy='mean',axis=0)\n",
    "imputer=imputer.fit(X2[:,7:12])\n",
    "X2[:,7:12]=imputer.transform(X2[:,7:12])\n",
    "X2=X2[:,:-1]\n",
    "from sklearn.preprocessing import LabelEncoder,OneHotEncoder\n",
    "categorical=[0,1,4,5,6]\n",
    "for i in categorical:\n",
    "    labelencoder=LabelEncoder()\n",
    "    X2[:,i]=labelencoder.fit_transform(X2[:,i])\n",
    "\n",
    "onehotencoder=OneHotEncoder(categorical_features=[0,1,4,5,6])\n",
    "X2=onehotencoder.fit_transform(X2).toarray()\n",
    "#X2=X2[:,:-1]\n",
    "X_train=X2[:-1234,:]\n",
    "X_test=X2[6019:,:]\n",
    "from sklearn.preprocessing import StandardScaler\n",
    "sc = StandardScaler()  \n",
    "X_train = sc.fit_transform(X_train)\n",
    "X_test=sc.fit_transform(X_test)\n",
    "y=sc.fit_transform(y)\n",
    "\n",
    "from keras.models import Sequential\n",
    "from keras.layers import Dense\n",
    "\n",
    "#Initializing Neural Network\n",
    "classifier = Sequential()\n",
    "\n",
    "#Adding the Input Layer and First Hidden Layer\n",
    "classifier.add(Dense(units=12, kernel_initializer='uniform', activation='relu', input_dim=62))\n",
    "\n",
    "#Adding the Second Hidden Layer\n",
    "classifier.add(Dense(units=12, kernel_initializer='uniform', activation='relu'))\n",
    "\n",
    "#Adding the Output Layer\n",
    "classifier.add(Dense(units=1, kernel_initializer='uniform'))#, activation='sigmoid'))\n",
    "\n",
    "#Compiling Neural Network\n",
    "classifier.compile(optimizer='adam', loss='mean_squared_error', metrics=['accuracy'])\n",
    "\n",
    "#Fitting Our Model\n",
    "classifier.fit(X_train, y, batch_size=100, epochs=1000)\n",
    "\"\"\"from sklearn.svm import SVR\n",
    "classifier = SVR(kernel = \"rbf\")\n",
    "classifier.fit(X_train, y)\"\"\"\n",
    "y_pred = classifier.predict(X_test)\n",
    "y_pred=sc.inverse_transform(y_pred)\n",
    "y_pred=np.log(y_pred)\n",
    "from decimal import Decimal\n",
    "for i in range(1234):\n",
    "    \n",
    "    y_pred[i,0]=round(y_pred[i,0],2)\n",
    "\n",
    "writer = pd.ExcelWriter('solution.xlsx', engine='xlsxwriter')\n",
    "resu1=pd.DataFrame(y_pred, columns=['Price'])\n",
    "resu1.to_excel(writer, 'Sheet1',index=False)\n",
    "writer.save()\n",
    "    \n",
    "        \n",
    "    \n"
   ]
  }
